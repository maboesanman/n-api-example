{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch js",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "port": 9229
    },
    {
      "name": "Attach to cpp",
      "type": "cppdbg",
      "request": "attach",
      "program": "${workspaceFolder}/build/Debug/exampleaddon.node",
      "processId": "${command:pickProcess}",
      "MIMode": "lldb"
    }
  ],
  "compounds": [
    {
      "name": "Addon",
      "configurations": ["Launch js", "Attach to cpp"],
    }
  ]
}